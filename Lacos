//for(inicializador, encerrador, incrementador)
let i =0;
console.log(i);
for(let i=0; i<=5; i++)
{
    console.log(`Resultado da variável FOR ${i}`);
}

//while com a condição

let contador =0;

while(contador <=5)
{
    console.log(`Resultado da variável While ${contador}`);
    contador++;
}

//do while
let contador2 = 0;
do 
{
    console.log(`Resultado da variável DO ${contador2}`);
    contador2++;
} while (contador2<5);

